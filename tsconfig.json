{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "kolenergo-core": [
        "dist/kolenergo-core"
      ],
      "kolenergo-core/*": [
        "dist/kolenergo-core/*"
      ],
      "@kolenergo/core": [
        "dist/kolenergo-core"
      ],
      "@kolenergo/osr": [
        "projects/operative-situation/src/exports.ts"
      ],
      "@kolenergo/core/*": [
        "dist/kolenergo-core/*"
      ]
    }
  }
}
